<form action="/sessions/destroy" method="post">
  <input type="submit" value="Logout" />
</form>

<a class="button" href="/spaces/new">Spaces</a>
<% if @user %>
  <h1>Welcome, <%= @user.name %></h1>
<% end %>

<ul>
<% @spaces.each do |space| %>
  <li><%= space %><a href="/make_reservation/<%= space[0] %>">Book this Space</a></li>
  <% end %>
</ul>

<a class="button" href="/spaces/new">List a Space</a>




